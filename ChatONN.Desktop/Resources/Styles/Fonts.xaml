<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:system="clr-namespace:System;assembly=netstandard">


    <!--Font families-->
    <FontFamily x:Key="LatoRegular">pack://application;,,,/Resources/Fonts/#Lato Regular</FontFamily>
    <FontFamily x:Key="LineIcon">pack://application;,,,/Resources/Fonts/#LineIcons</FontFamily>
    <FontFamily x:Key="MaterialIcon">pack://application;,,,/Resources/Fonts/#Material Design Icons</FontFamily>

    
    <!--font sizes-->
    <system:Double x:Key="FontSizeSmall">12</system:Double>
    <system:Double x:Key="FontSizeRegular">16</system:Double>
    <system:Double x:Key="FontSizeLarge">20</system:Double>
    <system:Double x:Key="FontSizeXLarge">24</system:Double>
    
</ResourceDictionary>